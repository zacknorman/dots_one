{
   "author": "HackHands",
   "background": {
      "persistent": false,
      "scripts": [ "background-script.js" ]
   },
   "content_scripts": [ {
      "js": [ "content-script.js" ],
      "matches": [ "*://*.hackhands.com/*", "*://*.pluralsight.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Share the screen of your computer.",
   "externally_connectable": {
      "matches": [ "*://*.hackhands.com/*", "*://*.pluralsight.com/*" ]
   },
   "homepage_url": "https://www.hackhands.com/",
   "icons": {
      "128": "logo128.png",
      "16": "logo16.png",
      "48": "logo48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "34",
   "name": "HackHands screen sharing",
   "offline_enabled": true,
   "permissions": [ "desktopCapture" ],
   "short_name": "HH-ScreenShare",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.8",
   "web_accessible_resources": [ "icon.png" ]
}
